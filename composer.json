{
    "name": "gugglegum/php-amazon-mws",
    "type": "library",
    "description": "An open-source library to connect to Amazon's MWS web services in an object-oriented manner, with a focus on intuitive usage.",
    "license": "Apache-2.0",
    "keywords": [
        "API",
        "Amazon",
        "PHP"
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "gugglegum\\phpAmazonMWS\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0.0, <6.0.0"
    },
    "autoload-dev": {
        "psr-4": {
            "gugglegum\\phpAmazonMWS\\tests\\": "tests/src/"
        }
    }
}
